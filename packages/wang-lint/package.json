{
  "name": "wang-lint-cli",
  "version": "0.0.6",
  "description": "前端编码规范工程化脚手架",
  "bin": "./lib/cli.js",
  "main": "./lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run test",
    "test": "npm run build && jest",
    "build": "rm -rf lib & npm run copyfiles && tsc",
    "copyfiles": "copyfiles -a -u 1 \"src/config/**\" lib",
    "dev": "npm run copyfiles && tsc -w"
  },
  "files": [
    "lib/",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=20.17.0"
  },
  "keywords": [
    "wang",
    "lint",
    "cli"
  ],
  "homepage": "https://github.com/Front-end-coder/wang-spec",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Front-end-coder/wang-spec.git"
  },
  "author": "wang",
  "license": "ISC",
  "packageManager": "pnpm@10.6.5",
  "devDependencies": {
    "@types/node": "^22.13.11",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "typescript": "^4.1.2",
    "wang-eslint-config": "^0.0.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "chalk": "^4.1.2",
    "child_process": "^1.0.2",
    "command-exists": "^1.2.9",
    "commander": "6",
    "cross-spawn": "^7.0.6",
    "ejs": "^3.1.10",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "execa": "^5.0.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "glob": "^7.1.6",
    "inquirer": "^7.3.3",
    "is-docker": "^2.2.1",
    "lodash": "^4.17.21",
    "markdownlint": "^0.25.1",
    "markdownlint-rule-helpers": "^0.28.0",
    "ora": "^5.4.1",
    "prettier": "^3.5.3",
    "strip-ansi": "^6.0.1",
    "stylelint": "^14.16.1",
    "terminal-link": "^2.1.1",
    "text-table": "^0.2.0",
    "wang-eslint-config": "workspace:^",
    "wang-markdownlint-config": "workspace:^"
